 <table>
        <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Publication Date</th>
        <th>Is enabled</th>

        </tr>
    
    {% for book in books %}
    <tr>
    <td>{{book.id}}</td>
    <td>{{book.title}} </td>
    <td>{{book.publicationDate |date('Y-m-d') }}</td>
    <td>{{book.enabled}}</td>
    <td><a href="{{ path('app_deletebook', { 'id': book.id }) }}">Delete</a></td>
    </tr>
    {% endfor %}
    </table>